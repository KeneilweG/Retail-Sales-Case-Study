{
  "datasets": [
    {
      "name": "bf3e6583",
      "displayName": "Retail Sales Analysis",
      "queryLines": [
        "SELECT\r\n",
        "  COUNT(DISTINCT `Transaction ID`) AS number_of_transactions,\r\n",
        "  COUNT(DISTINCT `Customer ID`) AS number_of_customers,\r\n",
        "  SUM(`Total Amount`) AS total_amount,\r\n",
        "  SUM(`Quantity`) AS total_quantity,\r\n",
        "  SUM(`Price per unit`) AS total_price,\r\n",
        "\r\n",
        "  CASE \r\n",
        "    WHEN `age` BETWEEN 0 AND 15 THEN '01. 0-15: Kids'\r\n",
        "    WHEN `age` BETWEEN 16 AND 20 THEN '02. 16-20: Teens'\r\n",
        "    WHEN `age` BETWEEN 21 and 30 THEN '03. 21-30: Young Adults'\r\n",
        "    WHEN `age` BETWEEN 31 AND 40 THEN '04. 31-40: Adults'\r\n",
        "    ELSE '05. 41+: Seniors'\r\n",
        "  END AS age_group,\r\n",
        "  gender,\r\n",
        "  `Product Category`,\r\n",
        "\r\n",
        "  case \r\n",
        "    WHEN `total amount` BETWEEN 0 AND 100 then '01. <100: Low'\r\n",
        "    WHEN `total amount` BETWEEN 101 AND 500 THEN '02. 101-500: Medium'\r\n",
        "    ELSE '03. >500: High'\r\n",
        "  END AS revenue_group,\r\n",
        "\r\n",
        "  dayname(`Date`) AS day,\r\n",
        "  dayofweek(`Date`) AS day_of_week,\r\n",
        "  date_format(`Date`, 'MMMM') AS month_name,\r\n",
        "  month(`Date`) AS month,\r\n",
        "  year(`Date`) AS purchase_date\r\n",
        "  \r\n",
        "FROM workspace.default.retail_sales_dataset_1\r\n",
        "GROUP BY ALL\r\n",
        "LIMIT 1000;"
      ]
    },
